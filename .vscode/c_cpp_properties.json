{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Core/Inc",
                "${workspaceFolder}/Core/Src"
            ],
            "defines": [
                "_DEBUG",
                "__ARM_ARCH_7EM__=1",
                "__CORTEX_M7=1",
                "__THUMBEL__=1",
                "__ARMEL__=1",
                "__GNUC__",
                "__GNUC_MINOR__",
                "__GNUC_PATCHLEVEL__"
            ],
            "windowsSdkVersion": "10.0.22621.0",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compilerPath": "C:/Users/doomt/scoop/apps/gcc-arm-none-eabi/current/bin/arm-none-eabi-gcc.exe",
            "intelliSenseMode": "gcc-arm",
            "cStandard": "gnu17",
            "cppStandard": "gnu++20",
            "compilerArgs": [
                "-mcpu=cortex-m7",
                "-mthumb",
                "-mfpu=fpv5-d16",
                "-mfloat-abi=hard",
                "-fno-rtti",
                "-fno-exceptions",
                "-std=c++20",
                "-Wno-unknown-attributes",
                "-Wno-unused-template"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            }
        }
    ],
    "version": 4
}
